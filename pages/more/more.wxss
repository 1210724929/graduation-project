page{
  background: #f3f3f3;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 140rpx;
  margin-bottom: 100rpx;
}
.userinfo-avatar {
  display: block;
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaa;
}
.register_body{
  overflow: hidden;
  font-size: 30rpx;
  margin-top: 50rpx;
  
}
.regist_list_item{
  overflow: hidden;
  margin-top: 10rpx;
  margin-bottom: 30rpx;
}
.regist_left{
  width: 200rpx;
  height: 50rpx;
 margin-left: 20rpx;
  text-align: center;
  float: left;
}
.regist_right{
  width: 400rpx;
  height: 50rpx;
  margin-right: 100rpx;
  text-align: center;
  float: right;
  
}
.register_picker{
  border: solid 1rpx rgba(213, 230, 180, 0.5);
}
input{
  border:1rpx solid  rgba(213, 230, 180, 0.5);
  font-size: 25rpx;
}
.regist_but{
  margin: 50rpx 30rpx 20rpx 0rpx;
  width: 750rpx;
  display: flex;
  flex-direction: row;
}
.regist_but_left{
  width: 250rpx;
}
.regist_but_right{
  width: 250rpx;
}